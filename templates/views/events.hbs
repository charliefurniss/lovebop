<div class="container">
	<h1>Events</h1>
</div>

<div class="container">
	<!-- if events -->
	{{#if events}}
		{{#each events}}
		<h3>
			{{name}}
		</h3>
		<h4>
			{{location}}
		</h4>
		<h4>
			{{date}}
		</h4>
		<h4>
			{{start_at}}â€“{{end_at}}
		</h4>
		{{/each}}
	{{else}}
	<!-- else -->
	<h3 class="text-muted">There are no events yet.</h3>
	{{/if}}
</div>